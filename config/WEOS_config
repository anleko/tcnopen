TCPATH =
CROSS = ${CROSS_COMPILE}

ARCH = ${SYSTEM_ARCH}-linux
CPU =
TARGET_VOS = posix
TARGET_OS = LINUX

TCPREFIX = ${CROSS_COMPILE}
TCPOSTFIX =
DOXYPATH =

CFLAGS += -Wall -DPOSIX -D_GNU_SOURCE ${CPUFLAGS}
ifeq (${SYSTEM_ENDIAN},-l)
  CFLAGS += -DL_ENDIAN
else
  CFLAGS += -DB_ENDIAN
endif
LDFLAGS += -lrt ${CPULDFLAGS}
